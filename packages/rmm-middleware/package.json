{
  "name": "@skroyc/rmm-middleware",
  "version": "0.1.0",
  "description": "Reflective Memory Management (RMM) middleware for LangChain.js with learnable reranking",
  "keywords": [
    "langchain",
    "memory",
    "middleware",
    "reranker",
    "reinforcement-learning",
    "rmm"
  ],
  "author": "SkrOYC <oscar@ocmasesorias.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skrOYC/langchain-middlewares-callbacks-ts.git",
    "directory": "packages/rmm-middleware"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "peerDependencies": {
    "langchain": "^1.2.17",
    "@langchain/core": "^1.0.0",
    "@langchain/langgraph": "^1.1.3",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "zod": "^4.3.6",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "langchain": {
      "optional": false
    },
    "@langchain/core": {
      "optional": false
    },
    "@langchain/langgraph": {
      "optional": false
    },
    "@langchain/langgraph-checkpoint": {
      "optional": false
    },
    "zod": {
      "optional": false
    }
  },
  "devDependencies": {
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "tsup",
    "test": "bun test",
    "download:longmemeval": "bun scripts/download-longmemeval.ts --which all --out-dir ./data/longmemeval",
    "eval:longmemeval:agent": "bun scripts/run-agent-longmemeval.ts",
    "eval:longmemeval:official:s": "bun scripts/run-agent-longmemeval.ts --mode all --paper-strict=true --dataset ./data/longmemeval/longmemeval_s_cleaned.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --reflection-model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=true --prebuild-methods rmm --prebuild-speaker2=false --prebuild-all-before-evaluation=true --require-prebuild=true --allow-raw-fallback=false --strict-prebuild-coverage=1 --metrics-retrieval-source=topm --prebuild-concurrency 20 --eval-concurrency 20 --reflection-cache=true --reflection-cache-path ./data/longmemeval/cache/reflection-cache.jsonl --save-artifacts=true --out-dir ./artifacts/longmemeval-s-official",
    "eval:longmemeval:official:m": "bun scripts/run-agent-longmemeval.ts --mode all --paper-strict=true --dataset ./data/longmemeval/longmemeval_m_cleaned.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --reflection-model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=true --prebuild-methods rmm --prebuild-speaker2=false --prebuild-all-before-evaluation=true --require-prebuild=true --allow-raw-fallback=false --strict-prebuild-coverage=1 --metrics-retrieval-source=topm --prebuild-concurrency 20 --eval-concurrency 20 --reflection-cache=true --reflection-cache-path ./data/longmemeval/cache/reflection-cache.jsonl --save-artifacts=true --out-dir ./artifacts/longmemeval-m-official",
    "eval:longmemeval:official:oracle": "bun scripts/run-agent-longmemeval.ts --mode all --paper-strict=true --dataset ./data/longmemeval/longmemeval_oracle.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --reflection-model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=true --prebuild-methods rmm --prebuild-speaker2=false --prebuild-all-before-evaluation=true --require-prebuild=true --allow-raw-fallback=false --strict-prebuild-coverage=1 --metrics-retrieval-source=topm --prebuild-concurrency 20 --eval-concurrency 20 --reflection-cache=true --reflection-cache-path ./data/longmemeval/cache/reflection-cache.jsonl --save-artifacts=true --out-dir ./artifacts/longmemeval-oracle-official",
    "eval:longmemeval:official:s:phase2": "bun scripts/run-agent-longmemeval.ts --mode eval --paper-strict=true --dataset ./data/longmemeval/longmemeval_s_cleaned.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=false --require-prebuild=true --allow-raw-fallback=false --strict-prebuild-coverage=1 --metrics-retrieval-source=topm --vector-store-cache=true --vector-store-cache-dir ./data/longmemeval/cache/vector-stores.s1only.c20 --eval-concurrency 20 --save-artifacts=true --resume=false --out-dir ./artifacts/longmemeval-s-official-s1only-c20-eval",
    "eval:longmemeval:paper:prebuild:s": "bun scripts/run-agent-longmemeval.ts --mode prebuild --paper-strict=true --dataset ./data/longmemeval/longmemeval_s_cleaned.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --reflection-model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=true --prebuild-methods rmm --prebuild-speaker2=false --allow-raw-fallback=false --prebuild-concurrency 20 --save-artifacts=true --out-dir ./artifacts/longmemeval-s-paper-prebuild",
    "eval:longmemeval:paper:eval:s": "bun scripts/run-agent-longmemeval.ts --mode eval --paper-strict=true --dataset ./data/longmemeval/longmemeval_s_cleaned.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=false --require-prebuild=true --allow-raw-fallback=false --strict-prebuild-coverage=1 --metrics-retrieval-source=topm --vector-store-cache=true --vector-store-cache-dir ./data/longmemeval/cache/vector-stores.s1only.c20 --eval-concurrency 20 --save-artifacts=true --resume=true --out-dir ./artifacts/longmemeval-s-paper-eval",
    "eval:longmemeval:paper:all:s": "bun scripts/run-agent-longmemeval.ts --mode all --paper-strict=true --dataset ./data/longmemeval/longmemeval_s_cleaned.json --model-adapter ./scripts/adapters/openrouter-model-adapter.ts --reflection-model-adapter ./scripts/adapters/openrouter-model-adapter.ts --judge-adapter ./scripts/adapters/openrouter-judge-adapter.ts --embeddings-adapter ./scripts/adapters/openrouter-embeddings-adapter.ts --embedding-dimension 2048 --methods rmm,rag,oracle --prebuild-topic-bank=true --prebuild-methods rmm --prebuild-speaker2=false --prebuild-all-before-evaluation=true --require-prebuild=true --allow-raw-fallback=false --strict-prebuild-coverage=1 --metrics-retrieval-source=topm --prebuild-concurrency 20 --eval-concurrency 20 --reflection-cache=true --reflection-cache-path ./data/longmemeval/cache/reflection-cache.jsonl --save-artifacts=true --out-dir ./artifacts/longmemeval-s-paper-all",
    "check": "bunx biome check",
    "fix": "bunx biome check --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@langchain/anthropic": "^1.3.19",
    "@langchain/community": "^1.1.17",
    "@langchain/openai": "^1.2.10"
  }
}
